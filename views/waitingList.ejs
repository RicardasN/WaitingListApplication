<% include ./partials/header %>
<meta http-equiv="refresh" content="5">
<div class="ftco-cover-1 overlay" style="background-image: url('images/waitingLounge.jpg')">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-12">
                <div class="feature-car-rent-box-1">
                        <% if(success_message != ''){ %>
                            <div class="alert alert-success alert-dismissible" role="alert">
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
                                        aria-hidden="true">&times;</span></button>
                                <%=success_message%>
                            </div>
                        <% } %>
                        <% if(error_message != ''){ %>
                            <div class="alert alert-danger alert-dismissible" role="alert">
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
                                        aria-hidden="true">&times;</span></button>
                                <%=error_message%>
                            </div>
                        <% } %>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">Name</th>
                                <th scope="col">Specialist</th>
                                <th scope="col">Time Remaining</th>
                                <th scope="col"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i=0; i<clients.length; i++){ %>
                            <tr>
                                <th scope="row"><%= clients[i].client_id %></th>
                                <td><%= clients[i].name %></td>
                                <td><%= clients[i].sepcialist_name %></td>
                                <% if(clients[i].servingTime){%>
                                <td><%= moment(clients[i].ticketCreated).add(clients[i].servingTime*clients[i].rowPosition, 'minutes').fromNow() %></td>
                                <%}else{%>
                                    <td><%= moment(clients[i].ticketCreated).add(5*clients[i].rowPosition, 'minutes').fromNow() %></td>
                                <%}%>
                            </tr>
                            <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<% include ./partials/footer %>